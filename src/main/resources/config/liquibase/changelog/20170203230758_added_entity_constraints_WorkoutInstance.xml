<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity WorkoutInstance.
    -->
    <changeSet id="20170203230758-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="workout_template_id"
                                 baseTableName="workout_instance"
                                 constraintName="fk_workout_instance_workout_template_id"
                                 referencedColumnNames="id"
                                 referencedTableName="workout_template"/>

        <addForeignKeyConstraint baseColumnNames="workout_instances_id"
                                 baseTableName="workout_instance_exercise"
                                 constraintName="fk_workout_instance_exercise_workout_instances_id"
                                 referencedColumnNames="id"
                                 referencedTableName="workout_instance"/>
        <addForeignKeyConstraint baseColumnNames="exercises_id"
                                 baseTableName="workout_instance_exercise"
                                 constraintName="fk_workout_instance_exercise_exercises_id"
                                 referencedColumnNames="id"
                                 referencedTableName="exercise"/>
        
    </changeSet>
</databaseChangeLog>
