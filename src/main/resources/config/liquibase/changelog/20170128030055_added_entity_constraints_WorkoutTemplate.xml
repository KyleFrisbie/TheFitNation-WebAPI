<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity WorkoutTemplate.
    -->
    <changeSet id="20170128030055-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_demographic_id"
                                 baseTableName="workout_template"
                                 constraintName="fk_workout_template_user_demographic_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_demographic"/>

        <addForeignKeyConstraint baseColumnNames="workout_templates_id"
                                 baseTableName="workout_template_workout_instance"
                                 constraintName="fk_workout_template_workout_instance_workout_templates_id"
                                 referencedColumnNames="id"
                                 referencedTableName="workout_template"/>
        <addForeignKeyConstraint baseColumnNames="workout_instances_id"
                                 baseTableName="workout_template_workout_instance"
                                 constraintName="fk_workout_template_workout_instance_workout_instances_id"
                                 referencedColumnNames="id"
                                 referencedTableName="workout_instance"/>
        
    </changeSet>
</databaseChangeLog>
