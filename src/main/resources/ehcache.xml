    <cache alias="com.thefitnation.domain.WorkoutTemplate.workoutInstances" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.WorkoutInstance" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.WorkoutInstance.workoutLogs" uses-template="simple"/>

