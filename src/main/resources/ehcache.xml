    <cache alias="com.thefitnation.domain.WorkoutLog.workoutTemplates" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.Exercise.workoutInstances" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
