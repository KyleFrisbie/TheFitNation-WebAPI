    <cache alias="com.thefitnation.domain.UserDemographic.gyms" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.UserDemographic.workoutInstances" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.UserDemographic.workoutTemplates" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.UserWeight" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.Gym" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.Gym.userDemographics" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.WorkoutTemplate" uses-template="simple"/>

