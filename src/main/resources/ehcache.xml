<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="com.thefitnation.domain.User" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.Authority" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.thefitnation.domain.PersistentToken" uses-template="simple"/>

